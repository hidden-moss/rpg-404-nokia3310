config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "AudioLoader",
"language": "GDScript",
"path": "res://script/manager/audio_loader.gd"
} ]
_global_script_class_icons={
"AudioLoader": ""
}

[application]

config/name="RPG 404 - Nokia 3310"
run/main_scene="res://data/scenes/Main.tscn"
boot_splash/image="res://data/sprites/splash.png"
boot_splash/use_filter=false
config/icon="res://icon.png"

[autoload]

Events="*res://script/manager/mgr_event.gd"
K="*res://script/manager/const.gd"
G="*res://script/manager/global.gd"
MgrNft="*res://script/manager/mgr_nft.gd"
AudioManager="*res://data/autoload/AudioManager.gd"

[debug]

settings/fps/force_fps=15

[display]

window/size/width=840
window/size/height=480
window/size/resizable=false
window/dpi/allow_hidpi=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"

